{
  "name": "bullet-proof-gas",
  "version": "0.0.17",
  "description": "",
  "packageManager": "pnpm@8.7.0",
  "keywords": [
    "javascript",
    "ethereum",
    "eip1559",
    "ethers",
    "web3",
    "dapp"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/inaridiy/eip1559-fee-calcculator.git"
  },
  "homepage": "https://github.com/inaridiy/eip1559-fee-calcculator.git",
  "files": [
    "dist",
    "!dist/**/*.tsbuildinfo",
    "src/**/*.ts",
    "!src/**/*.test.ts",
    "!src/**/*.test-d.ts",
    "!src/**/*.bench.ts"
  ],
  "author": "inaridiy",
  "license": "MIT",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf dist",
    "build": "pnpm run clean && tsc ",
    "format": "rome format . --write",
    "lint": "rome check .",
    "lint:fix": "pnpm lint --apply"
  },
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./core": {
      "default": "./dist/core/index.js",
      "types": "./dist/core/index.d.ts"
    },
    "./ethers5": {
      "default": "./dist/ethers-adapter/ethersV5.js",
      "types": "./dist/ethers-adapter/ethersV5.d.ts"
    },
    "./ethers6": {
      "default": "./dist/ethers-adapter/ethersV6.js",
      "types": "./dist/ethers-adapter/ethersV6.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "core": [
        "./dist/core/index.d.ts"
      ],
      "ethers5": [
        "./dist/ethers-adapter/ethersV5.d.ts"
      ],
      "ethers6": [
        "./dist/ethers-adapter/ethersV6.d.ts"
      ]
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "peerDependencies": {
    "ethers-v5": "npm:ethers@>=5.0.0 <6.0.0",
    "ethers-v6": "npm:ethers@>=6.0.0 <7.0.0",
    "viem": "^>=1.0.0 <2.0.0",
    "typescript": ">=5.2.2"
  },
  "peerDependenciesMeta": {
    "ethers-v5": {
      "optional": true
    },
    "ethers-v6": {
      "optional": true
    },
    "viem": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.2",
    "rome": "12.1.3",
    "typescript": "^5.2.2",
    "rimraf": "^5.0.1"
  }
}
